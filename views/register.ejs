<%- include('./partials/header.ejs') %>

<h1 style="text-align: center">Sign Up</h1>
<div class="container">
  <div class="row">
    <div style="width: 30%; margin: 30px auto">
      <form action="/register" method="POST">
        <div class="form-group">
          <input
            class="form-control"
            type="text"
            name="username"
            placeholder="username"
          />
        </div>
        <div class="form-group">
          <input
            class="form-control"
            type="password"
            name="password"
            placeholder="password"
          />
        </div>
        <!-- <button>Submit</button> -->
        <div class="form-group">
          <button class="btn btn-lg btn-default btn-block btn-primary">
            Submit!
          </button>
        </div>
      </form>
      <a href="/villas"><b>Go Back!</b></a>
    </div>
  </div>
</div>
